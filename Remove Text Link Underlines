/* Remove Underlines From Body Text Links */
.sqs-block-content p a, .sqs-block-content p a:visited {border: none !important;
}

/* Remove Underlines From H1 Heading Links */
.sqs-block-content h1 a, .sqs-block-content h1 a:visited {border: none !important;
}

/* Remove Underlines From H2 Heading Links */
.sqs-block-content h2 a, .sqs-block-content h2 a:visited {border: none !important;
}

/* Remove Underlines From H3 Heading Links */
.sqs-block-content h3 a, .sqs-block-content h3 a:visited {border: none !important;
}
